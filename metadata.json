{
  "name": "dhoppe-ssh",
  "version": "1.0.4",
  "author": "Dennis Hoppe",
  "summary": "This module installs, configures and manages the SSH service.",
  "license": "Apache-2.0",
  "source": "git://github.com/dhoppe/puppet-ssh.git",
  "project_page": "https://github.com/dhoppe/puppet-ssh",
  "issues_url": "https://github.com/dhoppe/puppet-ssh/issues",
  "dependencies": [
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 4.5.0 < 5.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "6",
        "7",
        "8"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "12.04",
        "14.04"
      ]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": "3.x"
    }
  ],
  "tags": [
    "ssh",
    "debian",
    "ubuntu"
  ]
}
